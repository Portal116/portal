package ch05;

import java.util.Scanner;

public class Q3 {
	static int N;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		int[] a = new int[N];
		
		for(int i=0;i<N;i++) {
			a[i] = sc.nextInt();
		}
		
		int result = gcdArray(a,0,N);
		
		System.out.println(result);
	}
	static int gcdArray(int[] a, int start, int n) {
		if(n == 1)
			return a[0];
		else if(n == 2)
			return gcd(a[0], a[1]);
		else {
			return gcd(a[0], gcdArray(a, start+1, n-1));
		}	
	}
	static int gcd(int x, int y) {
		if(y == 0)
			return x;
		else return gcd(y, x%y);
	}
}