package ch03;

import java.util.*;

public class p128 {
	static class PhyscData{
		private String name;
		private int height;
		private double vision;
		public PhyscData(String name, int height, double vision) {
			this.name = name;
			this.height = height;
			this.vision = vision;
		}
		@Override
		public String toString() {
			// TODO Auto-generated method stub
			return name + " " + height + " " + vision;
		}
		
	}
}
