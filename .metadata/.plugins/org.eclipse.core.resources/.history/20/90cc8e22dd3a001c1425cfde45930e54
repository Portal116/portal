package ch01;

public class Var01 {
	public static void main(String[] args) {
		ChildVar cv = new ChildVar();
		int intA;
		intA = 50;
		//intA = "문자열";
		String sA;
		sA = "문자열";
		
		ChildVar cs;
		
		byte ba;
		ba = 90;
		cs = new ChildVar();
	}
}

class ChildVar{
	int cc; //4byte
	double dd; //8byte
}

